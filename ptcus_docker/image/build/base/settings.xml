                    <id>ptcu-central</id>
                    <name>plugins-release</name>
                    <url>http://maven.ptclms.com/artifactory/plugins-release</url>
                </pluginRepository>
                <pluginRepository>
                    <snapshots/>
                    <id>ptcu-snapshots</id>
                    <name>plugins-snapshot</name>
